rm -f $HOME/carta.log
rm -f $HOME/carta.err

srun -N 1 -o $HOME/carta.log -e $HOME/carta.err --partition=windfall --account=windfall $HOME/bin/carta_start.sh

sleep 2s

# get line of interest
# put new line in front of http
# get lines beginning with http
# get the line upto a space
# keep only unique lines
grep "CARTA is accessible at" carta.log | sed 's/http/\nhttp/g' | grep ^http | sed 's/\(^http[^ <]*\)\(.*\)/\1/g' | sort -u

squeue -u joarlewi | grep carta